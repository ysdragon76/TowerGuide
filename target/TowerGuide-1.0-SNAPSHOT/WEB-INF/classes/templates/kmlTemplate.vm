            </BalloonStyle>
        </Style>
        <StyleMap id='Style2-point-1-map'>
            <Pair>
                <key>normal</key>
                <styleUrl>#Style2-point-1</styleUrl>
            </Pair>
            <Pair>
                <key>highlight</key>
                <styleUrl>#Style2-point-1-hover</styleUrl>
            </Pair>
        </StyleMap>
