            </Pair>
        </StyleMap>
        #foreach($result in $resultList)
            <Placemark>
                <name><![CDATA[$result.Operatorname]]></name>
                <snippet></snippet>
                <description><![CDATA[<div class="googft-info-window">
                    <img height="50" width="200" src="https://static1.squarespace.com/static/57fc29bb197aea4a8e83c18b/t/57fc2b09bebafbb2078d2aaa/1524841251323/?format=1500w"/><br>
                    <b>Timestamp:</b> $result.Timestamp<br>
                    <b>Longitude:</b> $result.Longitude<br>
                    <b>Latitude:</b> $result.Latitude<br>
                    <b>Operatorname:</b> $result.Operatorname<br>
                    <b>Level:</b> $result.Level
                </div>]]></description>
                <styleUrl>
                    #if($result.Level > -150 and $result.Level < -100)
                        #Style2-point-3-map
                    #elseif($result.Level > -100 and $result.Level < -90)
                        #Style2-point-4-map
                    #elseif($result.Level > -90 and $result.Level < -80)
                        #Style2-point-2-map
                    #elseif($result.Level > -80 and $result.Level < -70)
                        #Style2-point-1-map
                    #elseif($result.Level > -70 and $result.Level < 0)
                        #Style2-point-5-map
                    #else
                        #Style2-point-1-map
                    #end
                </styleUrl>
                <Point>
                    <coordinates>$result.Longitude,$result.Latitude,0</coordinates>
                </Point>
            </Placemark>
        #end
    </Document>
</kml>